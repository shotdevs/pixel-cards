{
    "name": "pixel-cards",
    "version": "3.1.2",
    "description": "Beautiful, customizable Discord card generators - Music cards, Welcome cards, User profiles, and Guild status cards with full color and background customization",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.mjs",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist",
        "fonts",
        "examples"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts --clean"
    },
    "keywords": [
        "discord",
        "discord-cards",
        "discord-bot",
        "canvas",
        "musicard",
        "music-card",
        "welcome-card",
        "user-card",
        "guild-status",
        "server-status",
        "discord-welcome",
        "discord-canvas",
        "customizable",
        "card-generator",
        "pixel-cards"
    ],
    "author": "shotxd01",
    "license": "GPL-3.0-only",
    "devDependencies": {
        "@types/node": "^20.11.28",
        "tsup": "^8.0.2",
        "typescript": "^5.4.2"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/shotdevs/pixel-cards"
    },
    "dependencies": {
        "@napi-rs/canvas": "^0.1.51"
    }
}