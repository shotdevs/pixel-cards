{
    "name": "pixel-cards",
    "version": "3.0.9",
    "description": "A custom music card and Discord server status generator for the Pixel music bot.",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.mjs",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist",
        "fonts"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts --clean"
    },
    "keywords": [
        "musicard",
        "discord",
        "canvas",
        "guild-status",
        "server-status",
        "activity-card",
        "ghost-x-bot",
        "welcome-card",
        "discord-welcome",
        "member-welcome"
    ],
    "author": "shotxd01",
    "license": "GPL-3.0-only",
    "devDependencies": {
        "@types/node": "^20.11.28",
        "tsup": "^8.0.2",
        "typescript": "^5.4.2"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/shotdevs/pixel-cards"
    },
    "dependencies": {
        "@napi-rs/canvas": "^0.1.51"
    }
}